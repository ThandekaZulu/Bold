<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/fragmentGetStarted">

    <fragment
        android:id="@+id/fragmentGetStarted"
        android:name="com.view.FragmentGetStarted"
        android:label="fragment_get_started"
        tools:layout="@layout/fragment_get_started" >
        <action
            android:id="@+id/action_fragmentGetStarted_to_fragmentUserDetails"
            app:destination="@id/fragmentUserDetails" />
    </fragment>
    <fragment
        android:id="@+id/fragmentUserDetails"
        android:name="com.view.FragmentUserDetails"
        android:label="fragment_user_details"
        tools:layout="@layout/fragment_user_details" >
        <action
            android:id="@+id/action_fragmentUserDetails_to_fragmentAvatar"
            app:destination="@id/fragmentAvatar" />
    </fragment>
    <fragment
        android:id="@+id/fragmentAvatar"
        android:name="com.view.FragmentAvatar"
        android:label="fragment_avatar"
        tools:layout="@layout/fragment_avatar" >
        <action
            android:id="@+id/action_fragmentAvatar_to_fragmentCompleteOnboarding"
            app:destination="@id/fragmentCompleteOnboarding" />
    </fragment>
    <fragment
        android:id="@+id/fragmentCompleteOnboarding"
        android:name="com.view.FragmentCompleteOnboarding"
        android:label="fragment_complete_onboarding"
        tools:layout="@layout/fragment_complete_onboarding" >
        <action
            android:id="@+id/action_fragmentCompleteOnboarding_to_fragmentDashboard"
            app:destination="@id/fragmentDashboard" />
    </fragment>
    <fragment
        android:id="@+id/fragmentDashboard"
        android:name="com.view.FragmentDashboard"
        android:label="fragment_dashboard"
        tools:layout="@layout/fragment_dashboard" >
        <action
            android:id="@+id/action_fragmentDashboard_to_fragmentPaperOneLanguage"
            app:destination="@id/fragmentPaperOneLanguage" />
        <action
            android:id="@+id/action_fragmentDashboard_to_fragmentPaperTwoReading"
            app:destination="@id/fragmentPaperTwoReading" />
        <action
            android:id="@+id/action_fragmentDashboard_to_fragmentPaperThreeWriting"
            app:destination="@id/fragmentPaperThreeWriting" />
        <action
            android:id="@+id/action_fragmentDashboard_to_fragmentPaperFourOralsAndListening"
            app:destination="@id/fragmentPaperFourOralsAndListening" />
    </fragment>
    <fragment
        android:id="@+id/fragmentPaperOneLanguage"
        android:name="com.view.FragmentPaperOneLanguage"
        android:label="fragment_paper_one_language"
        tools:layout="@layout/fragment_paper_one_language" />
    <fragment
        android:id="@+id/fragmentPaperTwoReading"
        android:name="com.view.FragmentPaperTwoReading"
        android:label="fragment_paper_two_reading"
        tools:layout="@layout/fragment_paper_two_reading" />
    <fragment
        android:id="@+id/fragmentPaperThreeWriting"
        android:name="com.view.FragmentPaperThreeWriting"
        android:label="fragment_paper_three_writing"
        tools:layout="@layout/fragment_paper_three_writing" />
    <fragment
        android:id="@+id/fragmentPaperFourOralsAndListening"
        android:name="com.view.FragmentPaperFourOralsAndListening"
        android:label="fragment_paper_four_orals_and_listening"
        tools:layout="@layout/fragment_paper_four_orals_and_listening" />
</navigation>